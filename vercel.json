{
  "version": 2,
  "buildCommand": "npm run build:production",
  "outputDirectory": "build",
  "functions": {
    "api/index.ts": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/:host([^/]*\\.[^/]*)",
      "destination": "https://template-fmcg.vercel.app/:host"
    }
  ],
  "routes": [
    {
      "src": "/api/.*",
      "dest": "/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}