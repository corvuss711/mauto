{
  "version": 2,
  "buildCommand": "npm run build:production",
  "outputDirectory": "build",
  "functions": {
    "api/index.ts": {
      "memory": 512,
      "maxDuration": 10
    }
  },
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "/api/index.ts"
    },
    {
      "source": "/about-us",
      "destination": "/index.html"
    },
    {
      "source": "/contact-us",
      "destination": "/index.html"
    },
    {
      "source": "/gallery",
      "destination": "/index.html"
    },
    {
      "source": "/services",
      "destination": "/index.html"
    },
    {
      "source": "/login",
      "destination": "/index.html"
    },
    {
      "source": "/signup",
      "destination": "/index.html"
    },
    {
      "source": "/auto-site",
      "destination": "/index.html"
    },
    {
      "source": "/solutions/:path(.*)",
      "destination": "/index.html"
    },
    {
      "source": "/access-denied",
      "destination": "/index.html"
    },
    {
      "source": "/attendance-leave-management",
      "destination": "/index.html"
    },
    {
      "source": "/order-management-solution",
      "destination": "/index.html"
    },
    {
      "source": "/field-force-tracking",
      "destination": "/index.html"
    },
    {
      "source": "/distributor-management-solution",
      "destination": "/index.html"
    },
    {
      "source": "/merchandising-retail-execution",
      "destination": "/index.html"
    },
    {
      "source": "/expenses-claims-management",
      "destination": "/index.html"
    },
    {
      "source": "/retailer-management-solution",
      "destination": "/index.html"
    },
    {
      "source": "/sales-activity-management",
      "destination": "/index.html"
    },
    {
      "source": "/purchase-order-management",
      "destination": "/index.html"
    },
    {
      "source": "/store-management-software",
      "destination": "/index.html"
    },
    {
      "source": "/production-management",
      "destination": "/index.html"
    },
    {
      "source": "/packing-management",
      "destination": "/index.html"
    },
    {
      "source": "/demand-generation",
      "destination": "/index.html"
    },
    {
      "source": "/invoice-generation-solution",
      "destination": "/index.html"
    },
    {
      "source": "/dispatch-management",
      "destination": "/index.html"
    },
    {
      "source": "/plant-management",
      "destination": "/index.html"
    },
    {
      "source": "/hrms",
      "destination": "/index.html"
    },
    {
      "source": "/website-development-services",
      "destination": "/index.html"
    },
    {
      "source": "/crm-software",
      "destination": "/index.html"
    },
    {
      "source": "/digital-marketing-services",
      "destination": "/index.html"
    },
    {
      "source": "/whatsapp-ordering-system",
      "destination": "/index.html"
    },
    {
      "source": "/:host([^/]+)/api",
      "destination": "/api/index.js"
    },
    {
      "source": "/:host([^/]+)/api/:endpoint(.*)",
      "destination": "/api/:endpoint"
    },
    {
      "source": "/:host([^/]+)",
      "destination": "https://template-fmcg.vercel.app/:host"
    },
    {
      "source": "/:host([^/]+)/:path(.*)",
      "destination": "https://template-fmcg.vercel.app/:host/:path"
    },
    {
      "source": "/((?!api/|_next/|assets/|uploads/|.*\\..*).*)",
      "destination": "/index.html"
    }
  ]
}