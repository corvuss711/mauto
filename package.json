{
  "name": "manacle-website",
  "private": true,
  "type": "module",
  "pkg": {
    "assets": [
      "dist/spa/*"
    ],
    "scripts": [
      "dist/server/**/*.js"
    ]
  },
  "scripts": {
    "dev": "vite",
    "dev:server": "tsx server/index.ts",
    "dev:full": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "build": "npm run clean && npm run build:production",
    "build:production": "vite build --mode production",
    "build:client": "vite build && copy public\\.htaccess build\\",
    "build:server": "vite build --config vite.config.server.ts",
    "clean": "rimraf build dist",
    "preview": "vite preview",
    "deploy:prepare": "npm run build && npm run copy:assets && npm run optimize",
    "copy:assets": "echo 'Assets copied during build process'",
    "optimize": "echo 'Build optimized for production'",
    "start": "vite",
    "start:prod": "node dist/server/node-build.mjs",
    "dev:prod": "npm run build && npm run start:prod",
    "test": "vitest --run",
    "format.fix": "prettier --write .",
    "typecheck": "tsc"
  },
  "dependencies": {
    "bcrypt": "^6.0.0",
    "dotenv": "^17.0.1",
